{
  "uid": "agent-runner-logs",
  "title": "Agent Runner Logs",
  "tags": ["agent-runner"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-1h", "to": "now" },
  "templating": {
    "list": [
      {
        "name": "kind",
        "label": "Kind",
        "type": "query",
        "datasource": { "type": "loki", "uid": "loki" },
        "query": "label_values({job=\"agent-runner\"}, kind)",
        "current": { "text": "task-run", "value": "task-run" },
        "refresh": 1,
        "sort": 1,
        "multi": false,
        "includeAll": false
      },
      {
        "name": "contains",
        "label": "Contains",
        "type": "textbox",
        "query": "",
        "current": { "text": "", "value": "" }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "logs",
      "title": "Logs",
      "datasource": { "type": "loki", "uid": "loki" },
      "gridPos": { "h": 22, "w": 24, "x": 0, "y": 0 },
      "options": {
        "showTime": true,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{job=\"agent-runner\", kind=~\"$kind\"} |= \"$contains\""
        }
      ]
    }
  ]
}
